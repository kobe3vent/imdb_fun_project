{
  "name": "imdb-fun-project",
  "version": "0.0.1",
  "description": "playing with imdb api",
  "author": "cecilarthurhk@gmail.com",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "typescript",
    "nestjs",
    "template"
  ],
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build --path tsconfig.prod.json",
    "start": "node dist/main.js",
    "dev": "nest start --watch --debug=0.0.0.0:9229",
    "test": "rimraf coverage .nyc_output && concurrently 'npm:test:unit' 'npm:test:e2e' && yarn calculate-global-test-coverage",
    "test:unit": "vitest run --coverage --config vitest.config.unit.ts",
    "test:e2e": "vitest run --coverage --config ./vitest.config.e2e.ts",
    "test:performance": "k6 run tests/performance/contexts/users/get-users.mjs",
    "calculate-global-test-coverage": "tsx scripts/calculate-global-test-coverage.ts",
    "prepare": "husky",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.mjs,cjs,.ts,.mts",
    "lint:fix": "yarn lint -- --fix",
    "lint:yaml": "chmod +x scripts/lint_yaml.sh && ./scripts/lint_yaml.sh",
    "typos": "chmod +x scripts/check_typos.sh && ./scripts/check_typos.sh"
  },
  "dependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/platform-fastify": "^10.4.6",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "pdfkit-table": "^0.1.99",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@nestjs/schematics": "^10.2.2",
    "@nestjs/testing": "^10.4.6",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.39",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.8.2",
    "@types/pdfkit": "^0.13.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-istanbul": "^1.3.1",
    "concurrently": "^9.0.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-vitest": "^0.4.1",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nock": "^13.5.5",
    "nyc": "^17.1.0",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "unplugin-swc": "^1.5.1",
    "vite": "^5.4.10",
    "vitest": "^1.3.1",
    "vitest-mock-extended": "^1.3.1"
  }
}
